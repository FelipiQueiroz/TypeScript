class Animal {
  falar(): void {
    console.log("O animal está fazendo um som.");
  }
}

class Cachorro extends Animal {
  falar(): void {
    console.log("Au Au");
  }
}

// Exemplo de uso
const meuAnimal = new Animal();
meuAnimal.falar(); // Saída: O animal está fazendo um som.

const meuCachorro = new Cachorro();
meuCachorro.falar(); // Saída: Au Au
